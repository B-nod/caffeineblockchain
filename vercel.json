{
    "version":2,
    "builds": [
        {
        "src": "caffeine/wsgi.py",
        "use": "@vercel/python"
       
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config": {
                "disDir": "staticfiles_build"
            }
        }
],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "caffeine/wsgi.py"
        }
    ]
}

